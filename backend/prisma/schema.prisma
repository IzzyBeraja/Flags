generator client {
  provider = "prisma-client-js"
  log      = "(array)"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL_DEV")
  relationMode = "prisma"
}

model User {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}
